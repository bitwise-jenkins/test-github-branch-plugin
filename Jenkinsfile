pipeline {
  agent none
  stages {
    stage('Build') {
      steps {
        echo 'BUILD_ID: ${env.BUILD_ID}'
        echo 'GIT_COMMIT: ${env.GIT_COMMIT}'
        echo 'GIT_PREVIOUS_COMMIT: ${env.GIT_PREVIOUS_COMMIT}'
        echo 'Hello 0d'
        echo 'Hello 1'
        echo 'Hello 2'
        echo 'Hello 4'
        echo 'Hello 5'
      }
    }
  }
}